{
  "name": "@rehearsal-ai/snippet",
  "version": "1.0.0",
  "description": "Client snippet that injects a floating bug button and modal",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "esbuild src/index.ts --bundle --format=iife --global-name=Bugger --outfile=dist/bugger.min.js --minify --sourcemap",
    "dev": "esbuild src/index.ts --bundle --format=iife --global-name=Bugger --outfile=dist/bugger.min.js --watch",
    "clean": "rimraf dist"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@rehearsal-ai/server": "*",
    "esbuild": "^0.19.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0",
    "zod": "^3.22.0"
  }
}
